{
  "dashboard": {
    "id": null,
    "title": "ISX Pulse - Scratch Card License Monitoring",
    "tags": ["isx", "license", "scratch-card", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Scratch Card Activation Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(scratch_card_activations_total[5m]))",
            "legendFormat": "Activations/sec",
            "refId": "A"
          },
          {
            "expr": "sum(rate(scratch_card_activations_total{card_type=\"STANDARD\"}[5m]))",
            "legendFormat": "Standard Cards/sec",
            "refId": "B"
          },
          {
            "expr": "sum(rate(scratch_card_activations_total{card_type=\"PREMIUM\"}[5m]))",
            "legendFormat": "Premium Cards/sec",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Success Rate by Card Type",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(scratch_card_success_rate{card_type=\"STANDARD\"})",
            "legendFormat": "Standard",
            "refId": "A"
          },
          {
            "expr": "avg(scratch_card_success_rate{card_type=\"PREMIUM\"})",
            "legendFormat": "Premium",
            "refId": "B"
          },
          {
            "expr": "avg(scratch_card_success_rate{card_type=\"ENTERPRISE\"})",
            "legendFormat": "Enterprise",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 85},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Apps Script Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(apps_script_response_time_seconds_bucket[5m])) by (le))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(apps_script_response_time_seconds_bucket[5m])) by (le))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(apps_script_response_time_seconds_bucket[5m])) by (le))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "s",
            "min": 0,
            "label": "Response Time"
          }
        ],
        "alert": {
          "conditions": [
            {
              "query": {"queryType": "", "refId": "B"},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [5], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "Apps Script High Response Time",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Device Fingerprint Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(fingerprint_validations_total[5m])",
            "legendFormat": "Validations/sec",
            "refId": "A"
          },
          {
            "expr": "rate(fingerprint_mismatches_total[5m])",
            "legendFormat": "Mismatches/sec",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(fingerprint_generation_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile generation time",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0,
            "label": "Rate"
          },
          {
            "unit": "s",
            "min": 0,
            "label": "Duration"
          }
        ],
        "seriesOverrides": [
          {
            "alias": "95th percentile generation time",
            "yAxis": 2
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Rate Limiting & Security Events",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(apps_script_rate_limits_total[5m])",
            "legendFormat": "Apps Script Rate Limits",
            "refId": "A"
          },
          {
            "expr": "rate(license_rate_limit_hits_total[5m])",
            "legendFormat": "License Rate Limits",
            "refId": "B"
          },
          {
            "expr": "rate(license_security_events_total[5m])",
            "legendFormat": "Security Events",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "reqps",
            "min": 0,
            "label": "Events/sec"
          }
        ],
        "alert": {
          "conditions": [
            {
              "query": {"queryType": "", "refId": "A"},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [0.1], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "for": "1m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Rate Limiting Activity",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "Batch Processing Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(batch_activations_total[5m])",
            "legendFormat": "Batches Processed/sec",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(batch_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile processing time",
            "refId": "B"
          },
          {
            "expr": "avg(batch_failure_rate)",
            "legendFormat": "Failure Rate %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0,
            "label": "Rate"
          },
          {
            "unit": "s",
            "min": 0,
            "label": "Duration"
          }
        ],
        "seriesOverrides": [
          {
            "alias": "95th percentile processing time",
            "yAxis": 2
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Error Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (failure_type) (rate(scratch_card_failures_by_type_total[5m]))",
            "legendFormat": "{{failure_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "displayMode": "table"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 8,
        "title": "System Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"isx-license-service\"}",
            "legendFormat": "Service Up",
            "refId": "A"
          },
          {
            "expr": "apps_script_connectivity",
            "legendFormat": "Apps Script Connected",
            "refId": "B"
          },
          {
            "expr": "license_sheets_connectivity",
            "legendFormat": "Google Sheets Connected",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "Down"},
              {"type": "value", "value": "1", "text": "Up"}
            ]
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 9,
        "title": "Active Pending Cards",
        "type": "graph",
        "targets": [
          {
            "expr": "active_pending_cards",
            "legendFormat": "Pending Cards",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0,
            "label": "Count"
          }
        ],
        "alert": {
          "conditions": [
            {
              "query": {"queryType": "", "refId": "A"},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [1000], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Pending Card Count",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "card_type",
          "type": "query",
          "query": "label_values(scratch_card_activations_total, card_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "time_range",
          "type": "interval",
          "query": "1m,5m,15m,30m,1h,6h,12h,1d",
          "refresh": 2,
          "current": {"text": "5m", "value": "5m"}
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    }
  }
}